<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>
        <from>.+?/css/(.+?)_.+?\.css$</from>
        <to>/css/$1.css</to>
    </rule>
    <rule>
        <from>.+?/js/(.+?)_.+?\.js$</from>
        <to>/js/$1.js</to>
    </rule>
    <rule>
        <from>.+?/translations/(.+?)/(.+?)-.+?\.json$</from>
        <to>/translations/$1/$2.json</to>
    </rule>
</urlrewrite>
